//para Genexus
&jsonString = "["

for &item in &MyCollection
    if &jsonString <> "["
        &jsonString += ","
    endif
    &jsonString += !"{" + !"\"Campo1\":\"" + &item.Campo1.Trim() + !"\"" + ","
    &jsonString += !"\"Campo2\":\"" + &item.Campo2.Trim() + !"\"" + "}"
endfor

&jsonString += "]"



//java embedded
ConnectRabbitmq.publishJsonArrayFromString(&Connection, &RabbitConfig, &jsonString);